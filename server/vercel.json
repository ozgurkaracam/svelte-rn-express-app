{
  "version": 2,
  "builds": [
      {
          "src": "./bin/www",
          "use": "@vercel/node"
      }
  ],
  "routes": [
    {
        "src": "/api/(.*)",
        "dest": "./routes/api/index.js"
    },
      {
          "src": "/(.*)",
          "dest": "./public/index.html"
      }
  ],
  "public":true
  
}